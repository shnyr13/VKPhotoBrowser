<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="padev.vkphotobrowser">
    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:icon="@mipmap/ic_launcher"
                 android:roundIcon="@mipmap/ic_launcher_round"
                 android:supportsRtl="true"
                 android:theme="@style/AppTheme">
        <activity android:name=".application.ui.activity.FriendsActivity"
                  android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".application.ui.activity.SignInActivity"
                  android:theme="@style/AppTheme">
        </activity>

        <activity android:name=".application.ui.activity.PhotoBrowserActivity"
                  android:theme="@style/AppTheme">
        </activity>

    </application>
</manifest>
